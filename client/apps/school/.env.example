# Environment Variables for School Client (Development)

# API Configuration
VITE_API_BASE_URL=/api/v1/tenant
VITE_AUTH_API_BASE_URL=/api/v2/school

# Tenant Configuration
VITE_MAIN_DOMAIN=<your-domain>
VITE_PUBLIC_SITE_URL=<your-site-url>

# Keycloak OIDC
VITE_KEYCLOAK_URL=<keycloak-url>
VITE_KEYCLOAK_REALM=<your-realm-name>
VITE_KEYCLOAK_CLIENT_ID=<your-client-id>

# Feature Flags (optional)
VITE_ENABLE_MOCK_API=false