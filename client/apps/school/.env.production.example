# Environment Variables for School Client (Production)

# API
VITE_API_BASE_URL=https://<api-domain>/api/v1/tenant
VITE_AUTH_API_BASE_URL=https://<api-domain>/api/v2/school

# Domain (for subdomain tenant parsing)
VITE_MAIN_DOMAIN=<your-domain>
VITE_PUBLIC_SITE_URL=https://<admin-subdomain>.<your-domain>

# Keycloak OIDC
VITE_KEYCLOAK_URL=https://<sso-domain>
VITE_KEYCLOAK_REALM=<your-realm-name>
VITE_KEYCLOAK_CLIENT_ID=<your-client-id>

VITE_ENABLE_MOCK_API=false