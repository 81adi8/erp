NODE_ENV=development
ENV_VALIDATION_MODE=warn
PORT=3000

DATABASE_URL=postgres://<db-user>:<db-password>@<db-host>:<db-port>/<db-name>
DB_SSL=false

REDIS_HOST=<redis-host>
REDIS_PORT=6379
REDIS_PASSWORD=<redis-password>
REDIS_TLS=false
ALLOW_REDIS_MOCK=false

JWT_ACCESS_SECRET=<your-64-char-random-hex-access-secret>
JWT_REFRESH_SECRET=<your-64-char-random-hex-refresh-secret>
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d

CORS_ORIGIN=http://localhost:5173,http://localhost:5174
ROOT_DOMAIN=localhost
CLIENT_URL=http://localhost:5173
SERVER_URL=http://localhost:3000

KEYCLOAK_URL=<keycloak-url>
KEYCLOAK_REALM=<your-realm-name>
KEYCLOAK_CLIENT_ID=<your-client-id>
KEYCLOAK_ADMIN_CLIENT_ID=<admin-cli-client-id>
KEYCLOAK_ADMIN_USERNAME=<admin-username>
KEYCLOAK_ADMIN_PASSWORD=<admin-password>
KEYCLOAK_CLIENT_SECRET=<your-client-secret>

INTERNAL_API_KEY=<your-internal-api-key>
INTERNAL_IP_ALLOWLIST=127.0.0.1,::1,localhost

DEBUG_MODE=false
SKIP_TENANT_VALIDATION=false